  <Tabs tabs="{{tabs}}" bindtabCheckColor="tabCheckColor">
     <view class="order" wx:for="{{orders}}" wx:key="order_id">
         <view class="order_number">
             <view class="font">订单编号</view>
             <view class="num">{{item.order_number}}</view>
         </view>
         <view class="order_price">
              <view class="font">订单价格</view>
             <view class="num">￥ {{item.order_price}}</view>
         </view>
         <view class="order_date">
             <view class="font">订单日期</view>
             <view class="num">{{item.create_time_cn}}</view>
         </view>
     </view>
  </Tabs>
